{
    "fields": [
        {
            "name": "start",
            "constraints": {
                "required": true,
                "type": "http://www.w3.org/2001/XMLSchema#string",
                "pattern": "\\A\\d{4}(((-(1[0-2]|0[1-9]))?)|(-(1[0-2]|0[1-9]))-(3[01]|[12][0-9]|0[1-9]))\\z"
            }
        },
        {
            "name": "end",
            "constraints": {
                "type": "http://www.w3.org/2001/XMLSchema#string",
                "pattern": "\\A\\d{4}(((-(1[0-2]|0[1-9]))?)|(-(1[0-2]|0[1-9]))-(3[01]|[12][0-9]|0[1-9]))\\z"
            }
        },
        {
            "name": "title",
            "constraints": {
                "required": true,
                "type": "http://www.w3.org/2001/XMLSchema#string"
            }
        },
        {
            "name": "description",
            "constraints": {
                "required": true,
                "type": "http://www.w3.org/2001/XMLSchema#string"
            }
        },
        {
            "name": "eventType",
            "constraints": {
                "required": true,
                "type": "http://www.w3.org/2001/XMLSchema#string",
                "pattern": "release|vulnerability|milestone|other|/i"
            }
        },
        {
            "name": "eventImportance",
            "constraints": {
                "required": true,
                "type": "http://www.w3.org/2001/XMLSchema#string",
                "pattern": "note|low|medium|high|critical/i"
            }
        },
        {
            "name": "referenceURI",
            "constraints": {
                "required": true,
                "type": "http://www.w3.org/2001/XMLSchema#anyURI"
            }
        },
        {
            "name": "imageURI",
            "constraints": {
                "type": "http://www.w3.org/2001/XMLSchema#anyURI"
            }
        },
        {
            "name": "mediaURIs",
            "constraints": {
                "type": "http://www.w3.org/2001/XMLSchema#string"
            }
        },
        {
            "name": "tags",
            "constraints": {
                "type": "http://www.w3.org/2001/XMLSchema#string",
                "pattern": "((\"[^\"]*\"|\\w[^\"(,|\n)]*),*)*"
            }
        }            
    ]
}
